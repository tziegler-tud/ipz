            <div class="dashboard-component dashboard-component--fullwidth dashboard-component-dataTable">
                <div class="dashboard-component-header">
                    Impfstoffwechsel
                </div>
                <div class="dashboard-subcomponent-container">
                    <div class="dashboard-subcomponent dashboard-subcomponent-amount">
                        <div class="dashboard-component-value">
                            <span class="dashboard-component-value__text">{{data.switch.total}}</span>
                        </div>
                        <div class="dashboard-component-label">
                            <span class="dashboard-component-label__text">Gesamt</span>
                        </div>
                    </div>
                    <div class="dashboard-subcomponent dashboard-subcomponent-table mdc-menu-surface--anchor">
                        <div class="mdc-data-table">
                            <div class="mdc-data-table__table-container">
                                <table class="mdc-data-table__table" aria-label="Dessert calories">
                                    <thead>
                                    <tr class="mdc-data-table__header-row">
                                        <th class="mdc-data-table__header-cell" role="columnheader" scope="col">Geplant</th>
                                        <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric" role="columnheader" scope="col">Wechsel auf</th>
                                        <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric" role="columnheader" scope="col">Erst/Zweit/Booster</th>
                                        <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric" role="columnheader" scope="col">Uhrzeit</th>
                                    </tr>
                                    </thead>
                                    <tbody class="mdc-data-table__content">
                                    {{#each data.switch.table}}
                                        <tr class="mdc-data-table__row">
                                            <th class="mdc-data-table__cell" scope="row">{{this.switch.originalTypeName}}</th>
                                            <td class="mdc-data-table__cell mdc-data-table__cell--numeric">{{this.switch.newTypeName}}</td>
                                            <td class="mdc-data-table__cell mdc-data-table__cell--numeric">{{#if this.booster}}Booster{{else}}{{#if this.second}}2.{{else}}1.{{/if}}{{/if}}</td>
                                            <td class="mdc-data-table__cell mdc-data-table__cell--numeric">{{transformTimeString this.timestamp}}</td>
                                            <td class="switch-entry-actions" data-index="{{@index}}" data-id="{{this.id}}"><button class="material-icons mdc-icon-button" aria-label="Options">more_vert</button></td>
                                        </tr>
                                        <div id="switch-entry-menu--{{@index}}" class="mdc-menu mdc-menu-surface switch-entry-menu">
                                            <ul class="mdc-deprecated-list" role="menu" aria-hidden="true" aria-orientation="vertical" tabindex="-1">
                                                <li id="switch-entry-edit--{{@index}}" class="mdc-deprecated-list-item" role="menuitem" data-action="edit">
                                                    <span class="mdc-deprecated-list-item__ripple"></span>
                                                    <span class="mdc-deprecated-list-item__text">Eintrag bearbeiten</span>
                                                </li>
                                                <li class="mdc-deprecated-list-divider" role="separator"></li>
                                                <li id="switch-entry-delete--{{@index}}" class="mdc-deprecated-list-item" role="menuitem" data-action="delete">
                                                    <span class="mdc-deprecated-list-item__ripple"></span>
                                                    <span class="mdc-deprecated-list-item__text switch-entry-menu__delete">Eintrag löschen</span>
                                                </li>
                                            </ul>
                                        </div>
                                    {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="switch-delete-dialog" class="mdc-dialog">
        <div class="mdc-dialog__container">
            <div class="mdc-dialog__surface"
                 role="alertdialog"
                 aria-modal="true"
                 aria-labelledby="my-dialog-title"
                 aria-describedby="my-dialog-content">
                <div class="mdc-dialog__content" id="my-dialog-content">
                    Eintrag löschen?
                </div>
                <div class="mdc-dialog__actions">
                    <button type="button" class="mdc-button mdc-dialog__button" data-mdc-dialog-action="cancel">
                        <div class="mdc-button__ripple"></div>
                        <span class="mdc-button__label">Abbrechen</span>
                    </button>
                    <button type="button" class="mdc-button mdc-dialog__button" data-mdc-dialog-action="delete">
                        <div class="mdc-button__ripple"></div>
                        <span class="mdc-button__label">Löschen</span>
                    </button>
                </div>
            </div>
        </div>
        <div class="mdc-dialog__scrim"></div>
    </div>
    <div id="switch-edit-dialog" class="mdc-dialog">
        <div class="mdc-dialog__container">
            <div class="mdc-dialog__surface"
                 role="alertdialog"
                 aria-modal="true"
                 aria-labelledby="my-dialog-title"
                 aria-describedby="my-dialog-content">
                <!-- Title cannot contain leading whitespace due to mdc-typography-baseline-top() -->
                <h2 class="mdc-dialog__title" id="my-dialog-title">Eintrag bearbeiten</h2>
                <div class="mdc-dialog__content" id="my-dialog-content">
                    <div class="mdc-deprecated-list-group">
                        <h3 class="mdc-list-group__subheader">Geplant:</h3>
                        <ul id="dialog-select-list1" class="mdc-deprecated-list mdc-deprecated-list--avatar-list">
                            <li class="mdc-deprecated-list-item" role="option" data-type="1" tabindex="-1">
                                <span class="mdc-list-item__ripple"></span>
                                <span class="mdc-deprecated-list-item__text">BionTech</span>
                            </li>

                            <li class="mdc-deprecated-list-item" role="option" data-type="2">
                                <span class="mdc-list-item__ripple"></span>
                                <span class="mdc-deprecated-list-item__text">Moderna</span>
                            </li>

                            <li class="mdc-deprecated-list-item" role="option" data-type="3">
                                <span class="mdc-list-item__ripple"></span>
                                <span class="mdc-deprecated-list-item__text">Astrazenecca</span>
                            </li>
                            <li class="mdc-deprecated-list-item" role="option" data-type="4">
                                <span class="mdc-list-item__ripple"></span>
                                <span class="mdc-deprecated-list-item__text">Johnson & Johnson</span>
                            </li>
                            <li class="mdc-deprecated-list-divider" role="separator"></li>
                        </ul>
                        <h3 class="mdc-list-group__subheader">Gewechselt auf:</h3>
                        <ul id="dialog-select-list2" class="mdc-deprecated-list mdc-deprecated-list--avatar-list">
                            <li class="mdc-deprecated-list-item" role="option" data-type="1" tabindex="0">
                                <span class="mdc-list-item__ripple"></span>
                                <span class="mdc-deprecated-list-item__text">BionTech</span>
                            </li>
                            <li class="mdc-deprecated-list-item" role="option" data-type="2">
                                <span class="mdc-list-item__ripple"></span>
                                <span class="mdc-deprecated-list-item__text">Moderna</span>
                            </li>
                            <li class="mdc-deprecated-list-item" role="option" data-type="3">
                                <span class="mdc-list-item__ripple"></span>
                                <span class="mdc-deprecated-list-item__text">Astrazenecca</span>
                            </li>
                            <li class="mdc-deprecated-list-item" role="option" data-type="4">
                                <span class="mdc-list-item__ripple"></span>
                                <span class="mdc-deprecated-list-item__text">Johnson & Johnson</span>
                            </li>
                            <li class="mdc-deprecated-list-divider" role="separator"></li>
                        </ul>
                        <h3 class="mdc-list-group__subheader">Erst-/ Zweitimpfung:</h3>
                        <ul id="dialog-select-list3" class="mdc-deprecated-list mdc-deprecated-list--avatar-list">
                            <li class="mdc-deprecated-list-item" role="option" data-second="false" tabindex="0">
                                <span class="mdc-list-item__ripple"></span>
                                <span class="mdc-deprecated-list-item__text">Erstimpfung</span>
                            </li>
                            <li class="mdc-deprecated-list-item" role="option" data-second="true">
                                <span class="mdc-list-item__ripple"></span>
                                <span class="mdc-deprecated-list-item__text">Zweitimpfung</span>
                            </li>
                            <li class="mdc-deprecated-list-divider" role="separator"></li>
                        </ul>
                    </div>
                </div>
                <div class="mdc-dialog__actions">
                    <button type="button" class="mdc-button mdc-dialog__button" data-mdc-dialog-action="close">
                        <div class="mdc-button__ripple"></div>
                        <span class="mdc-button__label">Abbrechen</span>
                    </button>
                    <button type="button" class="mdc-button mdc-dialog__button mdc-button--raised" data-mdc-dialog-action="accept">
                        <div class="mdc-button__ripple"></div>
                        <span class="mdc-button__label">Bestätigen</span>
                    </button>
                </div>
            </div>
        </div>
        <div class="mdc-dialog__scrim"></div>
    </div>
