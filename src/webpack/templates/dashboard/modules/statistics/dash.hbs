<div class="dashboard-module dashboard-module-dash management-dash">
    <div class="dashboard-inner dashboard-module-inner">
        <div class="dash-header">
            <span class="dash-header--date dash-header--title">{{transformDateStringExtended componentData.date}}</span>

        </div>
        <div class="dash-component-container">
            <div class="dash-component dash-component--full dashboard-component-dash management-dash">
                <div class="dash-component-inner">
                    <div class="dash-component-header">
                        <span>
                            Anzeige
                        </span>
                    </div>
                    <div class="dash-elements-container col">
                        <div class="dash-element">
                            <label for="select-date">Tag ausw채hlen:</label>
                            <select id="select-date" class="dash-header--date dash-header--title common-select">
                                <option>-- Tag ausw채hlen --</option>
                                {{#each componentData.archive}}
                                    <option value="{{#if this.current}}current{{else}}{{this.date}}{{/if}}" {{#ifEquals (getProperty (transformDate this.date) "date") (getProperty (transformDate ../componentData.date) "date") }}selected{{/ifEquals}}>{{this.date}}</option>
                                {{/each}}
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="dash-component-container">
            <div class="dash-component dash-component--half dashboard-component-dash management-dash">
                <div class="dash-component-inner">
                    <div class="dash-component-header">
                        <span>
                            Gesamtzahlen
                        </span>
                    </div>
                    <div class="dash-elements-container col">
                        <div class="dash-element half dash-element--figures">
                            <div class="totals">
                                <div class="totals-amount">
                                    <span>{{componentData.day.stats.total.all}}</span>
                                </div>
                                <div class="totals-label">
                                    <span>Durchgef체hrte Impfungen</span>
                                </div>
                            </div>
                            <div class="tracks">

                            </div>
                        </div>
                        <div class="dash-element half-auto dash-element--graph">
                            <div class="total-graph-content center">
                                <div id="totalChart-container" class="chart-container">
                                    <canvas id="totalChart"></canvas>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="dash-component dash-component--half dashboard-component-dash management-dash">
                <div class="dash-component-inner">
                    <div class="dash-component-header">
                        <span>
                            Impfungen pro Stunde
                        </span>
                    </div>
                    <div class="dash-elements-container col">
                        <div class="dash-element half-auto dash-element--figures">
                            <div class="totals">
                                <div class="totals-amount">
                                    <span>&empty;{{componentData.day.stats.average.total}}</span>
                                </div>
                                <div class="totals-label">
                                    <span>Gesamt</span>
                                </div>
                            </div>

                        </div>
                        <div class="dash-element full dash-element--graph">
                            <div class="average-graph-content">
                                <div id="averageChart-container" class="chart-container line-chart-container">
                                    <canvas id="averageChart"></canvas>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="dash-component dash-component--full dashboard-component-dash management-dash">
                <div class="dash-component-inner">
                    <div class="dash-component-header">
                        <span>
                            Wochen체bersicht
                        </span>
                    </div>
                    <div class="dash-elements-container col">
                        <div class="dash-element full dash-element--graph">
                            <div class="week-graph-content center">
                                <div id="weekChart-container" class="chart-container bar-chart-container">
                                    <canvas id="weekChart"></canvas>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="dash-component dash-component--full dashboard-component-dash management-dash">
                <div class="dash-component-inner">
                    <div class="dash-component-header">
                        <span>
                            Letzter Monat
                        </span>
                    </div>
                    <div class="dash-elements-container col">
                        <div class="dash-element full dash-element--graph">
                            <div class="month-graph-content center">
                                <div id="monthChart-container" class="chart-container bar-chart-container">
                                    <canvas id="monthChart"></canvas>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>