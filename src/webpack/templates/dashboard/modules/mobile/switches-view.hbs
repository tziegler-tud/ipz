<div class="dashboard dashboard-module dashboard-module-switches">
    <div class="dashboard-inner dashboard-module-inner">
        <div class="dashboard-header">
<!--            <span class="dashboard-header__text">{{header}}</span>-->
        </div>
        <div class="dashboard-component-container">
            <div class="dashboard-component dashboard-component--fullwidth dashboard-component-dataTable">
                <div class="dashboard-component-header">
                    Impfstoffwechsel
                </div>
                <div class="dashboard-subcomponent-container">
                    <div class="dashboard-subcomponent dashboard-subcomponent-amount">
                        <div class="dashboard-component-value">
                            <span class="dashboard-component-value__text">{{componentData.switch.total.all}}</span>
                        </div>
                        <div class="dashboard-component-label">
                            <span class="dashboard-component-label__text">Gesamt</span>
                        </div>
                    </div>
                    <div class="dashboard-subcomponent dashboard-subcomponent-table mdc-menu-surface--anchor">
                        <div id ="mdc-data-table" class="mdc-data-table">
                            <div class="mdc-data-table__table-container">
                                <table class="mdc-data-table__table" aria-label="Dessert calories">
                                    <thead>
                                    <tr class="mdc-data-table__header-row">
                                        <th class="mdc-data-table__header-cell" role="columnheader" scope="col">Geplant</th>
                                        <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric" role="columnheader" scope="col">Wechsel auf</th>
                                        <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric" role="columnheader" scope="col">1/2/Booster</th>
                                        <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric" role="columnheader" scope="col">Uhrzeit</th>
                                        <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric" role="columnheader" scope="col">Impfstrecke</th>
                                    </tr>
                                    </thead>
                                    <tbody class="mdc-data-table__content">
                                    {{#each componentData.switch.table}}
                                        <tr class="mdc-data-table__row">
                                            <th class="mdc-data-table__cell" scope="row">{{this.switch.originalTypeName}}</th>
                                            <td class="mdc-data-table__cell mdc-data-table__cell--numeric">{{this.switch.newTypeName}}</td>
                                            <td class="mdc-data-table__cell mdc-data-table__cell--numeric">{{#if this.booster}}Booster{{else}}{{#if this.second}}2.{{else}}1.{{/if}}{{/if}}</td>
                                            <td class="mdc-data-table__cell mdc-data-table__cell--numeric">{{transformTimeString this.timestamp}}</td>
                                            <td class="mdc-data-table__cell mdc-data-table__cell--numeric">{{this.track.name}}</td>
                                            <td class="switch-entry-actions" data-index="{{@index}}" data-id="{{this.id}}"><button class="material-icons mdc-icon-button" aria-label="Options">more_vert</button></td>
                                            <div id="switch-entry-menu--{{@index}}" class="mdc-menu mdc-menu-surface switch-entry-menu">
                                                <ul class="mdc-deprecated-list" role="menu" aria-hidden="true" aria-orientation="vertical" tabindex="-1">
                                                    <li id="switch-entry-edit--{{@index}}" class="mdc-deprecated-list-item" role="menuitem" data-action="edit">
                                                        <span class="mdc-deprecated-list-item__ripple"></span>
                                                        <span class="mdc-deprecated-list-item__text">Eintrag bearbeiten</span>
                                                    </li>
                                                    <li class="mdc-deprecated-list-divider" role="separator"></li>
                                                    <li id="switch-entry-delete--{{@index}}" class="mdc-deprecated-list-item" role="menuitem" data-action="delete">
                                                        <span class="mdc-deprecated-list-item__ripple"></span>
                                                        <span class="mdc-deprecated-list-item__text switch-entry-menu__delete">Eintrag l√∂schen</span>
                                                    </li>
                                                </ul>
                                            </div>
                                        </tr>

                                    {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>