<div class="mdc-data-table checkout-table">
    <div class="mdc-data-table__table-container checkout-table-container">
        <table class="mdc-data-table__table" aria-label="Dessert calories">
            <thead>
            <tr class="mdc-data-table__header-row">
                <th></th>
                <th class="mdc-data-table__header-cell" role="columnheader" scope="col">Nummer</th>
                <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric" role="columnheader" scope="col">Status</th>
                <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric" role="columnheader" scope="col">Zeit</th>
                <th class="mdc-data-table__header-cell" role="columnheader" scope="col">Comments</th>
            </tr>
            </thead>
            <tbody class="mdc-data-table__content">

            {{#each entries}}
                {{#each data}}
                    <tr data-entryid="{{../this.id}}" class="mdc-data-table__row checkout-table-row {{#if @first}}primary{{else}}secondary{{/if}}">
                        <td>{{#if @first}}{{else}}<span class="secondary-icon material-icons">add</span>{{/if}}</td>
                        <th class="mdc-data-table__cell" scope="row">{{this}}</th>
                        <td class="mdc-data-table__cell mdc-data-table__cell--numeric">WB1</td>
                        <td class="mdc-data-table__cell mdc-data-table__cell--numeric">{{transformTimeString ../this.currentStatus.timestamp}}</td>
                        <td class="mdc-data-table__cell">Super tasty</td>
                    </tr>
                {{/each}}
            {{/each}}
            </tbody>
        </table>
    </div>
</div>